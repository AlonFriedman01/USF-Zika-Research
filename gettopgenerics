#get top 20 images, mentions, tweets, users, countries,
library(plyr)
#have to have dplyr disabled, causes some conflict
detach(dplyr)

#most common countries
countrycount <- count(parsed_zika_12days$country)
countrycount <- na.omit(countrycount)
countrycount <- head(arrange(countrycount, desc(freq)), n = 20)

#most mentioned users
mentions <- count(parsed_zika_12days$mentioned_users)
mentions <- na.omit(mentions)
mentions <- head(arrange(mentions, desc(freq)), n = 20)

#most frequent tweeters
usrs <- count(parsed_zika_12days$screen_name)
usrs <- head(arrange(usrs, desc(freq)), n = 20)

#most common tweets
tweets <- count(parsed_zika_12days$text)
tweets <- head(arrange(tweets, desc(freq)), n = 20)

#most retweeted users
rt <- count(parsed_zika_12days$rt_screen_name)
rt <- na.omit(rt)
rt <- head(arrange(rt, desc(freq)), n = 20)

